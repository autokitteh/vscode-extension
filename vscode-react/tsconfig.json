{
	"compilerOptions": {
		"allowJs": false,
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"esModuleInterop": false,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"paths": {
			"@ak-proto-ts/*": ["../src/autokitteh/proto/gen/ts/autokitteh/*"],
			"@assets/*": ["assets/*"],
			"@api/*": ["../src/api/*"],
			"@api": ["../src/api/index"],
			"@constants": ["../src/constants/index"],
			"@constants/*": ["../src/constants/*"],
			"@enums": ["../src/enums/index"],
			"@enums/*": ["../src/enums/*"],
			"@interfaces": ["../src/interfaces/index"],
			"@interfaces/*": ["../src/interfaces/*"],
			"@i18n": ["../src/i18n/index"],
			"@i18n/*": ["../src/i18n/*"],
			"@models": ["../src/models/index"],
			"@models/*": ["../src/models/*"],
			"@react-assets": ["assets/index"],
			"@react-assets/*": ["assets/*"],
			"@react-components": ["src/components/index"],
			"@react-components/*": ["src/components/*"],
			"@react-constants": ["src/constants/index"],
			"@react-constants/*": ["src/constants/*"],
			"@react-enums": ["src/enums/index"],
			"@react-enums/*": ["src/enums/*"],
			"@react-hooks": ["src/hooks/index"],
			"@react-hooks/*": ["src/hooks/*"],
			"@react-interfaces": ["src/interfaces/index"],
			"@react-interfaces/*": ["src/interfaces/*"],
			"@react-sections": ["src/sections/index"],
			"@react-sections/*": ["src/sections/*"],
			"@react-utilities": ["src/utilities/index"],
			"@react-utilities/*": ["src/utilities/*"],
			"@sections/*": ["src/sections/*"],
			"@type": ["../src/types/index"],
			"@type/*": ["../src/types/*"],
			"@utilities": ["../src/utilities/index"],
			"@utilities/*": ["../src/utilities/*"],
			"@services": ["../src/services/index"],
			"@services/*": ["../src/services/*"],
			"@starlark": ["../src/starlark/index"],
			"@starlark/*": ["../src/starlark/*"],
			"@utils": ["../src/utilities/index"],
			"@utils/*": ["../src/utilities/*"]
		},
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext",
		"types": ["vite-plugin-svgr/client"],
		"useDefineForClassFields": true
	},
	"include": ["./src", "./tests/**/*.test.ts", "../src/enums/projectDeployment.enum.ts"]
}
